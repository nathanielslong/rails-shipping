<!DOCTYPE html>
<html>
  <head>
    <title>Ship to Shore</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    "https://cdn.datatables.net/1.10.13/css/dataTables.bootstrap.min.css" %>
    <%= stylesheet_link_tag    "https://fonts.googleapis.com/css?family=Montserrat" %>
    <%=stylesheet_link_tag "https://fonts.googleapis.com/css?family=Averia+Sans+Libre" %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag "https://cdn.datatables.net/1.10.13/js/jquery.dataTables.min.js" %>
    <%= javascript_include_tag "https://cdn.datatables.net/1.10.13/js/dataTables.bootstrap.min.js" %>
  </head>

  <body>
    <div class="background">
      <%= render 'layouts/header' %>
      <%= notice %>
      <%= alert %>
      <%= yield %>
      <br>
      <br>
      <br>
      <%= render 'layouts/footer' %>
    </div>
  </body>
</html>
